    <!-- Repair Tools -->
    <sys:String x:Key="ToolboxRemoveOffice">Usuń pakiet Office</sys:String>
    <sys:String x:Key="ToolboxRODescription">Jeśli nie można normalnie odinstalować pakietu Office, możesz spróbować go usunąć. Możesz spróbować wiele razy lub użyć innych metod.</sys:String>
    <sys:String x:Key="ToolboxRemoveViaOfficial">Usuń pakiet Office za pomocą oficjalnego narzędzia</sys:String>
