<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="Culture">en-us</sys:String>

    <sys:String x:Key="UpToDate">You are up to date!</sys:String>
    <sys:String x:Key="NewVersion">New version available.</sys:String>
    <sys:String x:Key="UpgradeNow">Upgrade</sys:String>
    <sys:String x:Key="Retry">Retry</sys:String>
    <sys:String x:Key="ForceShutdown">Force close OTP</sys:String>
    <sys:String x:Key="FixingApplication">File(s) missed, fixing application.</sys:String>
    <sys:String x:Key="Checking">Checking information…</sys:String>
    <sys:String x:Key="Downloading">Downloading file…</sys:String>
    <sys:String x:Key="Waiting">Please close OTP to continue.</sys:String>
    <sys:String x:Key="ExtractingFiles">Extracting files…</sys:String>
    <sys:String x:Key="NetworkError">Connection failed, please try again.</sys:String>
    <sys:String x:Key="DownloadError">Download failed</sys:String>
    <sys:String x:Key="CheckHashError">Checksum hash value failed.</sys:String>
    <sys:String x:Key="UpgradeError">Operate error!</sys:String>
    <sys:String x:Key="UpgradeSuccessful">Upgrade Successful</sys:String>
</ResourceDictionary>